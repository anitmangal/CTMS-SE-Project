<HomeWindow>
    name: "homew"
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        padding:50
        spacing:20
        background_color: (126/255.0, 198/255.5, 142/255.0, 1)
        canvas.before:
            Color:
                rgba: self.background_color
            Rectangle:
                pos: self.pos
                size: self.size
                source: 'assets/images/cground.jpg'
        Label:
            id: display_project
            text: "  [font=assets/fonts/Merriweather-Black]Cricket Tournament \n  Management System[/font]"
            text_size: self.size
            halign: 'left'
            valign: 'middle'
            
            background_color: (126/255.0, 198/255.5, 142/255.0, 0.45)
            canvas.before:
                Color:
                    rgba: self.background_color
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [18]
                    #source: 'assets/images/cground.jpg'

            #text properties:
            markup: True
            font_size: 50
            color: (1, 1, 1, 1)
            bold: True
            outline_color: (0, 0, 0)
            outline_width: 0

        RoundedButton:
            id: button_enter
            text: "Enter"
            font_size: 30
            bold: True
            size_hint: (1, 0.15)
            pos_hint: {'center_x': 0.5}
            on_release:
                app.root.current = "resetw"
                root.manager.transition.direction = "left"
            #background_normal: ''
            #background_color: (209/255.0, 217/255.0, 69/255.0, 0.75)

        MenuButton:
            id: aboutus
            text: "About us c:"
            font_size: 25
            bold: True
            size_hint: (1, 0.5)
            pos_hint: {'center_x': 0.5}            
            on_release:
                #root.setnames()
                app.root.current = "aboutusw"
                root.manager.transition.direction = "left"
<AboutUsWindow>
    name: "aboutusw"
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        padding:50
        spacing:20
        background_color: (126/255.0, 198/255.5, 142/255.0, 1)
        canvas.before:
            Color:
                rgba: self.background_color
            Rectangle:
                pos: self.pos
                size: self.size
                source: 'assets/images/englishcri.jpg'
        Label:

            text: "[font=assets/fonts/times]We are a team of three dedicated to creating a simple and efficient \nCricket Tournament Management System that stores and manages \ninformation related to players, teams, matches, \nand statistics. Our system allows for random generation of player \nand team details, generation of match schedules, \nand the ability to view player and team statistics, as well as \nmatch scorecards. Our system is designed to handle a round-robin \ntournament system with ease. This is a term-project for \nSoftware Engineering Lab 2023 made by \nAnit Mangal, Ratan Junior and Gorantla Thoyajakshi.[/font]"
            halign: 'center'
            valign: 'middle'
            background_color: (178/255.0, 21/255.0, 18/255.0 , 0.45)
            canvas.before:
                Color:
                    rgba: self.background_color
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [18]
            markup: True
            font_size: 27
            color: (1, 1, 1, 1)
            bold: True

        #go home
        RoundedButton:
            id: button_home2
            text: "go back home"
            font_size: 25
            bold: True
            size_hint: (0.5, 0.15)
            pos_hint: {'center_x': 0.5}            
            on_release:
                app.root.current = "homew"
                root.manager.transition.direction = "right"

<ResetWindow>
    name: "resetw"
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        padding:50
        spacing:20
        background_color: (126/255.0, 198/255.5, 142/255.0, 1)
        canvas.before:
            Color:
                rgba: self.background_color
            Rectangle:
                pos: self.pos
                size: self.size
                source: 'assets/images/cground.jpg'
        Label:
            text: "  [font=assets/fonts/Merriweather-Black]Do you want to reset the database?[/font]"
            font_size: 30
            background_color: (178/255.0, 21/255.0, 18/255.0 , 0.45)
            canvas.before:
                Color:
                    rgba: self.background_color
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [18]

            markup: True
            font_size: 25
            color: (1, 1, 1, 1)
            bold: True
        YesNoButton:
            text: "Yes"
            font_size: 30
            bold: True
            size_hint: (1, 0.30)
            pos_hint: {'center_x': 0.5}
            font_size: 24
            on_release:
                root.press_yes()
                app.root.current = "menuw"
                root.manager.transition.direction = "left"
        YesNoButton:
            text: "No"
            font_size: 30
            bold: True
            size_hint: (1, 0.30)
            pos_hint: {'center_x': 0.5}
            font_size: 24
            on_release:
                app.root.current = "menuw"
                root.manager.transition.direction = "left"

